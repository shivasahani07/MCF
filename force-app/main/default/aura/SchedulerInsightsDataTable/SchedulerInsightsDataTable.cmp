                                <div style="display: inline;"><b>Client Name:</b>&nbsp;</div><b style="color:#6592FD;">{!lead.Company}</b>
                                <div class="slds-text-align_left slds-has-flexi-truncate" title="{!lead.FirstName}"><b>Name:</b>&nbsp;{!lead.FirstName}&nbsp;{!lead.LastName}</div>
                                <div class="slds-text-align_left slds-has-flexi-truncate" style="white-space: nowrap; 
                                                                                                             width: 15rem; 
                                                                                                             overflow: hidden;
                                                                                                             text-overflow: ellipsis;"
                                     title="{!lead.Email}"><b>Email:</b>&nbsp;{!lead.Email}</div>
                                <div class="slds-text-align_left slds-has-flexi-truncate"
                                     title="{!lead.Start_Date_Time__c}"><b>Phone:</b>&nbsp;{!lead.Phone}</div>
                                <div class="slds-text-align_left slds-has-flexi-truncate"
                                     title="{!lead.Lead_Stage__c}"><b>Prospect Stage:</b>&nbsp;{!lead.Lead_Stage__c}</div>
                            </div>
                        </div>
                    </aura:iteration>
                </div>
                
            </lightning:tab>
            
            <lightning:tab label="Meeting" id="three">
