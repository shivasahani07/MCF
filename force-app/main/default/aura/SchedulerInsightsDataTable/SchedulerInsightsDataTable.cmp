                                        <td scope="row" data-label="Client Name">
                                            <div class="slds-text-align_left slds-has-flexi-truncate"
                                                 title="{!event.StartDateTime}">
                                                <lightning:formattedDateTime value="{!event.StartDateTime}" hour="2-digit" minute="2-digit"/></div>
                                        </td>
                                        <td scope="row" data-label="Client Name">
                                            <div class="slds-text-align_left slds-has-flexi-truncate"
                                                 title="{!event.EndDateTime}">
                                                <lightning:formattedDateTime value="{!event.EndDateTime}" hour="2-digit" minute="2-digit"/></div>
