                <div class="slds-col slds-size_6-of-12 " style="background-color:white;  width:98%; margin-top:-1s%;"  >
                    <lightning:icon iconName='standard:feedback' alternativeText='feedback' size='large' title='feedback' class="slds-m-around_small"></lightning:icon>
                    <div class=" ">
                        <table class="slds-table slds-table_bordered slds-table_cell-buffer slds-table_fixed-layout slds-table_striped">
                            <thead>
                                <tr class="" style="background-color:#9E7BFF;">
                                    <th scope="col" style="background-color:#6CA1E9;">
                                        <strong>
                                            <div class="slds-truncate slds-text-align_center slds-has-flexi-truncate"
                                                 title="S. No." style="color:#FFFFFF;">S. No.</div>
                                        </strong>
                                    </th>
                                    <th scope="col" style="background-color:#6CA1E9;">
                                        <strong>
                                            <div class="slds-truncate slds-text-align_left slds-has-flexi-truncate"
                                                 title="Name" style="color:#FFFFFF;">Feedback No.</div>
                                        </strong>
                                    </th>
                                    <th scope="col" style="background-color:#6CA1E9;">
                                        <strong>
                                            <div class="slds-truncate slds-text-align_left slds-has-flexi-truncate"
                                                 title="StageName" style="color:#FFFFFF;">Descriotion</div>
                                        </strong>
                                    </th>
                                    <th scope="col" style="background-color:#6CA1E9;">
                                        <strong>
                                            <div class="slds-truncate slds-text-align_left slds-has-flexi-truncate"
                                                 title="Close date" style="color:#FFFFFF;">Feedback Rating</div>
                                        </strong>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <aura:iteration items="{!v.FeedbackList}"  var="feedback" indexVar="index">
                                    <tr>
                                        
                                        <td scope="row" data-label="Name">
                                            <div class="slds-text-align_center"  >
                                                <div  aura:id="selectAllId" class="slds-truncate" title="">{!((v.FeedbackpageNumber-1)*10)+index+1}</div>     
                                            </div>
                                        </td>
                                        <td scope="row" data-label="Name">
                                            <div class="slds-text-align_left slds-has-flexi-truncate" >
                                                <b style="color:#6592FD;">{!feedback.Name}</b>
                                            </div>
                                        </td>
                                        <td scope="row" data-label="Route">
                                            <div class="slds-text-align_left slds-has-flexi-truncate" style="white-space: nowrap; 
                                                                                                             width: 16rem; 
                                                                                                             overflow: hidden;
                                                                                                             text-overflow: ellipsis;"
                                                 title="{!feedback.Description__c}">{!feedback.Description__c}</div>
                                        </td>
                                        <td scope="row" data-label="Name">
                                            <div class="slds-text-align_left slds-has-flexi-truncate">
                                                <b style="color:#6592FD;">{!feedback.Feedback_Rating__c}</b>
                                            </div>
                                        </td>
                                    </tr>
                                </aura:iteration>
                            </tbody>
                        </table>
                        <div class="slds-p-around-medium slds-align_absolute-center">
                            <lightning:buttonGroup class="paginationButton">
                                <lightning:button class="PrevButton" variant="brand" label="Prev" iconName="utility:chevronleft" iconPosition="left" onclick="{!c.FeedbackhandlePrev}" disabled="{! v.FeedbackpageNumber == 1}"/>
                                <lightning:button variant="brand" label="Next" iconName="utility:chevronright" iconPosition="right" onclick="{!c.FeedbackhandleNext}" disabled="{! v.FeedbackLastPage}"/>
                            </lightning:buttonGroup>
                        </div>
                    </div>
                </div>
                </div>
                
                <!--Card show CSS for Mobile-->
                <div class="BeatMobile">
                    <aura:iteration items="{!v.FeedbackList}"  var="feedback" indexVar="index">
                        <div class="slds-card ">
                            <div class="cardInfo">
                                <div class="slds-text-align_left slds-has-flexi-truncate" ><b>Feedback No.:</b>&nbsp;<b style="color:#6592FD;">{!feedback.Name}</b></div>
                                <div class="slds-text-align_left slds-has-flexi-truncate" style="white-space: nowrap; 
                                                                                                             width: 15rem; 
                                                                                                             overflow: hidden;
                                                                                                 text-overflow: ellipsis;"><b>Descriotion:</b>&nbsp;{!feedback.Description__c}</div>
                                <div class="slds-text-align_left slds-has-flexi-truncate"><b>Feedback Rating:</b>&nbsp;{!feedback.Feedback_Rating__c}</div>
                            </div>
                        </div>
                    </aura:iteration>
                </div>
                
            </lightning:tab>
        </lightning:tabset>
    </div>
</aura:component>