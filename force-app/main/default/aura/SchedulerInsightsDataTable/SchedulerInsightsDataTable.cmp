    <aura:attribute name="MeetingisLastPage" type="Boolean" default="false"/>
    <aura:attribute name="EmailsLastPage" type="Boolean" default="false"/>
    <aura:attribute name="ProposalsLastPage" type="Boolean" default="false"/>
    <aura:attribute name="NegotiationLastPage" type="Boolean" default="false"/>
    <aura:attribute name="QuotationLastPage" type="Boolean" default="false"/>
    <aura:attribute name="PurchaseOrderLastPage" type="Boolean" default="false"/>
    <aura:attribute name="InvoicesLastPage" type="Boolean" default="false"/>
    <aura:attribute name="TicketsLastPage" type="Boolean" default="false"/>
    <aura:attribute name="FeedbackLastPage" type="Boolean" default="false"/>
    
    <aura:attribute name="BeatStatusdataSize" type="Integer" default="0"/>
    <aura:attribute name="ProspectsdataSize" type="Integer" default="0"/>
    <aura:attribute name="MeetingissdataSize" type="Integer" default="0"/>
    <aura:attribute name="EmailsissdataSize" type="Integer" default="0"/>
    <aura:attribute name="ProposalsissdataSize" type="Integer" default="0"/>
    <aura:attribute name="NegotiationissdataSize" type="Integer" default="0"/>
    <aura:attribute name="QuotationissdataSize" type="Integer" default="0"/>
    <aura:attribute name="PurchaseOrderissdataSize" type="Integer" default="0"/>
    <aura:attribute name="InvoicesissdataSize" type="Integer" default="0"/>
    <aura:attribute name="TicketsissdataSize" type="Integer" default="0"/>
    <aura:attribute name="FeedbackissdataSize" type="Integer" default="0"/>
    
    <aura:attribute name="isLoading" type="Boolean" default="false"/>
    
    <div class="slds-m-left_medium slds-m-right_medium " >
