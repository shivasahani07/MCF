                                <lightning:button class="PrevButton" variant="brand" label="Prev" iconName="utility:chevronleft" iconPosition="left" onclick="{!c.BeatStatushandlePrev}" disabled="{! v.BeatStatuspageNumber == 1}"/>
                                <lightning:button variant="brand" label="Next" iconName="utility:chevronright" iconPosition="right" onclick="{!c.BeatStatushandleNext}" disabled="{! v.BeatStatusisLastPage}"/>
                            </lightning:buttonGroup>
                        </div>
                    </div>
                    
                    
                </div>
                
                <!--Card show CSS for Mobile-->
